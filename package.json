{
  "name": "moneylog",
  "version": "0.0.0",
  "main": "index.js",
  "author": {
    "name": "Alan Christian",
    "email": "alanchristiancreator@hotmail.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "fix": "run-s \"fix:* {@}\" --",
    "fix:lint": "eslint --fix --no-error-on-unmatched-pattern",
    "fix:format": "prettier --write",
    "format": "yarn fix:format 'src/**/*.(js|ts|md|json)",
    "lint": "yarn fix:lint src/**/*.{js,ts,md,json}",
    "test": "yarn jest",
    "prebuild": "rd /s /q dist",
    "build": "babel src --extensions \".js\" --ignore \"*.spec.js,*.test.js\" --out-dir dist --copy-files",
    "dev": "nodemon --exec babel-node -- -r dotenv-safe/config src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo ”[Husky] pre-commit”"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./src/**/*.{js,md,json}": [
      "yarn fix"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@prisma/client": "2.18.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "prisma": "2.18.0"
  }
}
